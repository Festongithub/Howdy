{
    "manifest_version": 2,
    "name": "Developer  Focus",
    "version": "1.0",
    "description": "Enable Developers and Users to enjoy browsing the internet",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
     "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["background.js"]
        }
    ]

}