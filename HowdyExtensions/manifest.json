{
    "name": "Howdy Extension",
    "manifest_version": 3,
    "version": "1.0",
    "description": "intercepts and replaces ad elements on web pages with customizable, positive content widgets",

    "action": {
        "default_popup": "popup/popup.html"
    },
    
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },

    "permissions": [
        "storage",
        "alarms"
    ]
}