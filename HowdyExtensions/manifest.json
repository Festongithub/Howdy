{
    "manifest_version": 3,
    "name": "Howdy Chrome Extension",
    "description" : "Provides a series of Quotes and Sports training for Developers",
    "version": "1.0",

    "action":{
        "default_popup":"popup/popup.html"
    },

    "background" : {
        "service_worker": "background.js",
        "type": "module"
    },

    "permissions": [
        "storage",
        "activeTab"
    ]
    
}